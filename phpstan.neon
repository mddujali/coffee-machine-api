includes:
    - vendor/larastan/larastan/extension.neon
    - vendor/nesbot/carbon/extension.neon

parameters:

    paths:
        - app/

    level: 5
    tmpDir: storage/phpstan/cache/

    ignoreErrors:
        - '#Match expression does not handle remaining value: mixed#'
